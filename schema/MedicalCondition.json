{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schema.org/MedicalCondition.json",
  "$comment": "Autogenerated with schema-org-gen",
  "title": "Medical Condition",
  "media": {
    "type": "application/json;profile=https://schema.org/MedicalCondition"
  },
  "allOf": [
    {
      "$ref": "MedicalEntity.json"
    }
  ],
  "type": "object",
  "properties": {
    "associatedAnatomy": {
      "$ref": "#/$defs/associatedAnatomy"
    },
    "differentialDiagnosis": {
      "$ref": "DDxElement.json#/$defs/possibleRefArray"
    },
    "drug": {
      "$ref": "Drug.json#/$defs/possibleRefArray"
    },
    "epidemiology": {
      "$ref": "#/$defs/epidemiology"
    },
    "expectedPrognosis": {
      "$ref": "#/$defs/expectedPrognosis"
    },
    "naturalProgression": {
      "$ref": "#/$defs/naturalProgression"
    },
    "pathophysiology": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/pathophysiology"
      }
    },
    "possibleComplication": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/possibleComplication"
      }
    },
    "possibleTreatment": {
      "$ref": "MedicalTherapy.json#/$defs/possibleRefArray"
    },
    "primaryPrevention": {
      "$ref": "MedicalTherapy.json#/$defs/possibleRefArray"
    },
    "riskFactor": {
      "$ref": "MedicalRiskFactor.json#/$defs/possibleRefArray"
    },
    "secondaryPrevention": {
      "$ref": "MedicalTherapy.json#/$defs/possibleRefArray"
    },
    "signOrSymptom": {
      "$ref": "MedicalSignOrSymptom.json#/$defs/possibleRefArray"
    },
    "stage": {
      "$ref": "MedicalConditionStage.json#/$defs/possibleRef"
    },
    "status": {
      "$ref": "#/$defs/status"
    },
    "typicalTest": {
      "$ref": "MedicalTest.json#/$defs/possibleRefArray"
    }
  },
  "$defs": {
    "arrayOfMedicalConditions": {
      "type": "array",
      "items": {
        "$ref": "#"
      }
    },
    "possibleRef": {
      "oneOf": [
        {
          "$ref": "#"
        },
        {
          "type": "string",
          "format": "uri",
          "links": [
            {
              "rel": "full",
              "href": "{+$}"
            }
          ]
        }
      ]
    },
    "possibleRefArray": {
      "oneOf": [
        {
          "type": "string",
          "format": "uri",
          "links": [
            {
              "rel": "full",
              "href": "{+$}"
            }
          ]
        },
        {
          "type": "array",
          "items": {
            "$ref": "#/$defs/possibleRef"
          }
        }
      ]
    },
    "associatedAnatomy": {
      "title": "Associated Anatomy",
      "description": "The anatomy of the underlying organ system or structures associated with this entity.",
      "anyOf": [
        {
          "$ref": "AnatomicalStructure.json#/$defs/possibleRef"
        },
        {
          "$ref": "SuperficialAnatomy.json#/$defs/possibleRef"
        },
        {
          "$ref": "AnatomicalSystem.json#/$defs/possibleRef"
        }
      ]
    },
    "epidemiology": {
      "title": "Epidemiology",
      "description": "The characteristics of associated patients, such as age, gender, race etc.",
      "type": "string"
    },
    "expectedPrognosis": {
      "title": "Expected Prognosis",
      "description": "The likely outcome in either the short term or long term of the medical condition.",
      "type": "string"
    },
    "naturalProgression": {
      "title": "Natural Progression",
      "description": "The expected progression of the condition if it is not treated and allowed to progress naturally.",
      "type": "string"
    },
    "pathophysiology": {
      "title": "Pathophysiology",
      "description": "Changes in the normal mechanical, physical, and biochemical functions that are associated with this activity or condition.",
      "type": "string"
    },
    "possibleComplication": {
      "title": "Possible Complication",
      "description": "A possible unexpected and unfavorable evolution of a medical condition. Complications may include worsening of the signs or symptoms of the disease, extension of the condition to other organ systems, etc.",
      "type": "string"
    },
    "status": {
      "title": "Status",
      "description": "The status of the study (enumerated).",
      "anyOf": [
        {
          "$ref": "EventStatusType.json#/$defs/possibleRef"
        },
        {
          "$ref": "MedicalStudyStatus.json#/$defs/possibleRef"
        },
        {
          "type": "string"
        }
      ]
    }
  }
}